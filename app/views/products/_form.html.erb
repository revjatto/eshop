<%= form_for @product do |f| %>

  <%= f.label :name %>:
  <%= f.text_field :name %><br />

  <%= f.label :price %>:
  <%= f.text_field :price %><br />
  
  <%= f.label :image_url %>:
  <%= f.text_field :image_url %><br />
  
  <%= f.label :stock %>:
  <%= f.text_field :stock %><br />

  <%= f.submit %>
<% end %>